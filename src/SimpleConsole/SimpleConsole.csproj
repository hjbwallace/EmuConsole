<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>SimpleConsole</PackageId>
    <Version>1.0.0</Version>
    <Authors>Harrison Wallace</Authors>
    <PackageTags>Console</PackageTags>
